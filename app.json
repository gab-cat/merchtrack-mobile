{
  "expo": {
    "name": "merchtrack-mobile",
    "slug": "merchtrack-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/logo_1024_white_t.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo/logo_1024_white_t.png",
      "resizeMode": "contain",
      "backgroundColor": "#2C59DB"
    },
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/logo_1024_white_t.png",
        "backgroundColor": "#2C59DB",
        "buildNumber": "1.0.0"
      },
      "package": "com.merchtrack.mobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/logo/logo_1024_white_t.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
